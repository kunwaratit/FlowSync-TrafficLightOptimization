{
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.3"
  },
  "scripts": {
    "build:css": "npx tailwindcss -i ./static/src/input.css -o ./static/src/output.css",
    "watch:css": "npx tailwindcss -i ./static/src/input.css -o ./static/src/output.css --watch",
    "start": "npm-run-all --parallel watch:*"
  },
  "dependencies": {
    "axios": "^1.7.2"
  }
}
